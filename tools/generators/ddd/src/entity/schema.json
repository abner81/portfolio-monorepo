{
  "$schema": "https://json-schema.org/schema",
  "$id": "EntityGenerator",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Entity Name",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    }
  },
  "project": {
    "type": "string",
    "description": "NX project name to insert entity",
    "$default": {
      "$source": "argv",
      "index": 1
    },
    "x-prompt": "What NX project?"
  },
  "context": {
    "type": "string",
    "description": "DDD Context",
    "$default": {
      "$source": "argv",
      "index": 2
    },
    "x-prompt": "What DDD context?"
  },
  "required": ["name", "project", "context"]
}
